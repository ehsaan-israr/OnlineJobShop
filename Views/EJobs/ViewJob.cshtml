@model List<AgainLastWebRTC.Models.jobsJ>
@{
    ViewBag.Title = "ViewJob";
    Layout = "~/Views/Shared/_LayoutJobss.cshtml";
}

<style>
    #capt {
    /*font-size:xx-large;*/
    font:bolder 25px arial, verdana;
    }
    .tableh {
    font:bolder 20px arial, verdana;
    

     }
    .tabled {
    text-align:match-parent;


     }
</style>

<div class="space20"></div>
<div class="page-head">
	<div class="container">
		<div class="row">
			<div class="col-md-6">
				<h3>POST JOBS</h3>
			</div>
			
		</div>
	</div>
</div>
<div class="space20"></div>
@foreach (var j in Model)
{
    <section class="container" style=" background-color:white;padding-bottom:2%;padding-top:2%;border-top-left-radius:50px;border-top-right-radius:50px;border-bottom-left-radius:50px;border-bottom-right-radius:50px;">
    <div class="container">
        <div class="caontainer" style="text-align:center;color:red;font-weight:bolder;"  >@j.title</div>
        <div style="float:left">@j.experience</div>
        <div style="float:left"> | </div>
        <div style="float:left">@j.qualification</div>
        <div style="float:left"> | </div>
        <div style="float:left">@j.description</div>
        <div style="float:left"> | </div>
        <div style="float:left;color:goldenrod" >@j.lastdate</div>
        <div style="float:left"> | </div>
         <div style="float:left">@j.salary</div>
        <div style="float:right"><a href="~/EJobs/DeleteJob/@j.Id">Delete</a></div>
        <div style="float:right"> | </div>
         <div style="float:right"><a href="~/EJobs/UpdateJob/@j.Id">Update</a></div>
        
    </div>
</section> 
    <div class="space10" ></div>

}
