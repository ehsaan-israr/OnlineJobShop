@*@model OnlineJobShop.Models.registerE*@


@model AgainLastWebRTC.Models.registerE
@{
    ViewBag.Title = "registerE";
}









<div class="Login">
    <div class="Login-head">
        <h3>Registration</h3>

    </div>
    @using (Html.BeginForm("registerEmployer", "Register", FormMethod.Post))
    {


        @Html.AntiForgeryToken()
        <div>
            <h4>Company Name</h4>
            @*@Html.ValidationMessageFor(model=>model.area)*@
            @Html.EditorFor(model => model.comName)<span> </span>
            <div class="clear"> </div>
        </div>
        
        <div>
            <h4>Email Address</h4>
            @*@Html.ValidationMessageFor(model=>model.email)*@
            @Html.EditorFor(model => model.eMailId)<span> </span>
            <div class="clear"> </div>
        </div>

        <div>
            <h4>Password</h4>
            @*@Html.ValidationMessageFor(model=>model.password)*@
            @Html.EditorFor(model => model.password)
            <span> </span>
            <div class="clear"> </div>
        </div>
        @*<h4>Confirm Password</h4>
            <input type="password" value="Password"  /><span> </span>
            <div class="clear"> </div>*@
        <div>
            <h4>Website</h4>
            @*@Html.ValidationMessageFor(model=>model.area)*@
            @Html.EditorFor(model => model.website)<span> </span>
            <div class="clear"> </div>
        </div>
               
        <div>
            <h4>Address</h4>
            @*@Html.ValidationMessageFor(model=>model.area)*@
            @Html.EditorFor(model => model.address)<span> </span>
            <div class="clear"> </div>
        </div>
        <div>
            <h4>Contact No.</h4>
            @*@Html.ValidationMessageFor(model=>model.contactno)*@
            @Html.EditorFor(model => model.contNum)<span> </span>
            <div class="clear"> </div>
        </div>
         @Html.RadioButtonFor(model => model.type,"Employer",new {id = "Employer"})  
        @Html.Label("Employer", "Employer")
        @Html.RadioButtonFor(model => model.type, "JobSeeker", new { id = "JobSeeker"})        
        @Html.Label("JobSeeker", "JobSeeker")
        

        <div class="submit-button">
            <input type="submit" value="Register  > ">
        </div>


    }


</div>










