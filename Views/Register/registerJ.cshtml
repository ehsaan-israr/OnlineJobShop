            @*@Html.ValidationMessageFor(model=>model.gender)*@
            @Html.EditorFor(model => model.gender)<span> </span>
            <div class="clear"> </div>
        </div>
        <div>
            <h4>Age </h4>
            @*@Html.ValidationMessageFor(model=>model.age)*@
            @Html.EditorFor(model => model.age)<span> </span>
            <div class="clear"> </div>
        </div>

        <div>
            <h4>Experience </h4>
            @*@Html.ValidationMessageFor(model=>model.age)*@
            @Html.EditorFor(model => model.experience)<span> </span>
            <div class="clear"> </div>
        </div>

        <div class="submit-button">
            <input type="submit" value="Register  > ">
        </div>


    }


</div>


