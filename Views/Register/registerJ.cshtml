@model AgainLastWebRTC.Models.registerJ
@{
    ViewBag.Title = "registerJ";
}

<br />
<div class="Login">
    <div class="Login-head">
        <h3>Registeration</h3>
    </div>


    @using (Html.BeginForm("registerJobseeker", "Register", FormMethod.Post))
    {


        @Html.AntiForgeryToken()


        <div>
            <h4>First Name</h4>
            @*@Html.ValidationMessageFor(model=>model.fname)*@
            @Html.EditorFor(model => model.fname)
